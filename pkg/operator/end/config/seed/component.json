{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "End Operator Component Spec",
  "type": "object",
  "required": ["body"],
  "additionalProperties": false,
  "properties": {
    "body": {
      "type": "object",
      "additionalProperties": false,
      "patternProperties": {
        "^[a-zA-Z_][a-zA-Z_0-9]*$": {
          "type": "object",
          "properties": {
            "value": {
              "type": "string"
            },
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
