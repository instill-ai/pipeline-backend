  "TASK_VECTOR_SEARCH": {
    "instillShortDescription": "Perform a vector search on a collection",
    "input": {
      "instillUIOrder": 0,
      "properties": {
        "collection-name": {
          "description": "The name of the collection to perform vector search on",
          "instillAcceptFormats": [
            "string"
