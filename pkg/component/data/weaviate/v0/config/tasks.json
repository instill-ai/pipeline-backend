          "instillUIOrder": 1,
          "instillUpstreamTypes": [
            "reference",
            "template",
            "value"
          ],
          "items": {
            "description": "A dimension of the vector",
            "example": 0.8167237,
            "type": "number"
          },
          "minItems": 1,
          "title": "Vector",
          "type": "array"
        },
        "limit": {
          "description": "The limit of objects, empty for all objects",
